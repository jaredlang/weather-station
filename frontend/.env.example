VITE_API_SERVICE_BASE_URL=http://localhost:8080
VITE_DEFAULT_CITY=Seattle

# Cache and retry configuration (in seconds)
# VITE_FORECAST_CACHE_DURATION: How long to cache forecast data (default: 120 seconds = 2 minutes)
# VITE_FORECAST_RETRY_DELAY: How long to wait before retrying failed requests (default: 120 seconds = 2 minutes)
VITE_FORECAST_CACHE_DURATION=120
VITE_FORECAST_RETRY_DELAY=120
