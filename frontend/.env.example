VITE_API_BASE_URL=http://localhost:8080
VITE_DEFAULT_CITY=Seattle

# Cache and retry configuration (in seconds)
# VITE_CACHE_DURATION: How long to cache forecast data (default: 120 seconds = 2 minutes)
# VITE_RETRY_DELAY: How long to wait before retrying failed requests (default: 120 seconds = 2 minutes)
VITE_CACHE_DURATION=120
VITE_RETRY_DELAY=120
